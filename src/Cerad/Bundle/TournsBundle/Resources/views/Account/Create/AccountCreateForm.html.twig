{# @CeradTourns/Account/Create/AccountCreateForm.html.twig   #}
{{ form_start(form, { 'attr' : { 
   'class' : 'cerad_tourn_account_create cerad_common_form1'
}}) }}
    <fieldset><legend><span>Account Information</span></legend>
        {{ form_row(form.fedKey)         }}
        {{ form_row(form.email)          }}
        {{ form_row(form.name)           }}
        {{ form_row(form.password.pass1) }}
        {{ form_row(form.password.pass2) }}
    </fieldset>
    <fieldset>
        <div class="controls">
            <span><a href="{{ path('cerad_tourn_welcome') }}">Return to Welcome</a></span>
            <input type="submit" name="accountCreate" value="Create Zayso Account" />
        </div>
    </fieldset>
{{ form_rest(form) }}
{{ form_end (form) }}


