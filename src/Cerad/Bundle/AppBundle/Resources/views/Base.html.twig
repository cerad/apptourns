{# @CeradApp/Base.html.twig #}
<!doctype html>
<html lang="en" ng-app="zaysoApp">
<head>
    <meta charset="utf-8"/>
    <meta name="description" content="Cerad Referee Scheduling"/>
    <meta name="author" content="Art Hundiak"/>
    
    <title>{% block title %}ArbiterTourn{% endblock %}</title>
    
    <link rel="icon" type="image/x-icon" href="{{ asset('bundles/ceradapp/images/favicon.ico') }}" />
    
    <link id="prefix" href="/arbiter/games" />
    
    <script src="{{ asset('bundles/ceradapp/js/angular.min.js') }}"></script>
    <script src="{{ asset('bundles/ceradapp/js/angular-resource.min.js') }}"></script>
    <script src="{{ asset('bundles/ceradapp/js/app.js') }}"></script>

{% block stylesheets %}{%endblock %} 

</head>

<body>
{% block layout_body %}   
  {% block layout_header   %}{%endblock %}     
  {% block layout_topmenu  %}{%endblock %}     
  {% block layout_userinfo %}{%endblock %}
  {% block layout_content  %}{%endblock %}
{%endblock %}
        
{% block javascripts %}{% endblock %}
</body>

</html>
